{
  "name": "thettler/laravel-factory-classes",
  "description": "A Package to easy create Factory Classes for your tests with a fluent api and automatic auto completion",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "thettler",
    "laravel-factory-classes"
  ],
  "homepage": "https://github.com/christophrumpel/laravel-factories-reloaded",
  "authors": [
    {
      "name": "Tobias Hettler",
      "email": "tobias.hettler@gmx.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4",
    "illuminate/support": "^5.8|^6.0|^7.0",
    "laravel/framework": "^5.8|^6.0|^7.0",
    "fzaninotto/faker": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.5",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Thettler\\LaravelFactoryClasses\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Thettler\\LaravelFactoryClasses\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Thettler\\LaravelFactoryClasses\\FactoryClassServiceProvider"
      ]
    }
  }
}
